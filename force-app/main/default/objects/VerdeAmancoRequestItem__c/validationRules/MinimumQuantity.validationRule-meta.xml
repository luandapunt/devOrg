<?xml version="1.0" encoding="UTF-8"?>
<ValidationRule xmlns="http://soap.sforce.com/2006/04/metadata">
    <fullName>MinimumQuantity</fullName>
    <active>true</active>
    <errorConditionFormula>AmountNmb__c  &lt;= 0</errorConditionFormula>
    <errorMessage>A quantidade de um item selecionado deve ser maior que zero.</errorMessage>
</ValidationRule>
