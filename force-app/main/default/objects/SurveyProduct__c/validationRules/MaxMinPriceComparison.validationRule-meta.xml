<?xml version="1.0" encoding="UTF-8"?>
<ValidationRule xmlns="http://soap.sforce.com/2006/04/metadata">
    <fullName>MaxMinPriceComparison</fullName>
    <active>true</active>
    <errorConditionFormula>MaxPriceNmb__c &lt; MinPriceNmb__c || 
MaxPriceNmb__c &lt; 0 || 
MinPriceNmb__c &lt; 0</errorConditionFormula>
    <errorMessage>Defina valores válidos para os preços mínimo e máximo do produto.</errorMessage>
</ValidationRule>
